Public Class start1_frm

    Public mysp, myrain As Integer

    Private Sub Form2_Load(sender As Object, e As EventArgs) Handles MyBase.Load
        mysp = 0
        myrain = 0
        cha_amanashi.ATKama = 1140
        cha_amanashi.HPama = 6986
    End Sub

    Private Sub start_frm_GotFocus(sender As Object, e As EventArgs) Handles Me.GotFocus
        Me.Visible = False
    End Sub

    Private Sub PictureBox1_Click(sender As Object, e As EventArgs) Handles PictureBox1.Click
        Me.Visible = False
        home_frm.Visible = True
    End Sub
End Class
