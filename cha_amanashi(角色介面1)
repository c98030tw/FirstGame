Public Class cha_amanashi
    Public hplv, atklv As Integer
    Public HPama, ATKama As Integer

    Private Sub PictureBox5_Click(sender As Object, e As EventArgs) Handles PictureBox5.Click
        Me.Visible = False
        allcha_frm.Visible = True
    End Sub

    Private Sub a01_Click(sender As Object, e As EventArgs) Handles a01.Click
        If start1_frm.mysp >= 1 Then
            start1_frm.mysp = start1_frm.mysp - 1
            ATKama = ATKama * 1.05
            a01.Visible = False
            ab01.Visible = False
            a02.Visible = True

            Label16.Visible = True
            Timer1.Enabled = True
        Else

            Label15.Visible = True
            Timer1.Enabled = True
        End If

        Label2.Text = ATKama
        Label4.Text = HPama
        L7.Text = start1_frm.mysp

    End Sub

    Private Sub a02_Click(sender As Object, e As EventArgs) Handles a02.Click
        If start1_frm.mysp >= 3 Then
